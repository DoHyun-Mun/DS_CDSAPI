    "DSOdataService.card01": {
      "kind": "entity",
      "query": {
        "SELECT": {
          "from": {
            "join": "inner",
            "args": [
              {
                "ref": [
                  "ds.api.cards.DS02"
                ],
                "as": "a"
              },
              {
                "ref": [
                  "ds.api.cards.DS01"
                ],
                "as": "b"
              }
            ],
            "on": [
              {
                "ref": [
                  "a",
                  "qcode"
                ]
              },
              "=",
              {
                "ref": [
                  "b",
                  "qcode"
                ]
              }
            ]
          },
          "columns": [
            {
              "ref": [
                "a",
                "asst_type2"
              ]
            },
            {
              "func": "sum",
              "args": [
                {
                  "ref": [
                    "b",
                    "cost1"
                  ]
                }
              ],
              "as": "cost1",
              "cast": {
                "type": "cds.Integer64"
              }
            },
            {
              "func": "sum",
              "args": [
                {
                  "ref": [
                    "b",
                    "cost2"
                  ]
                }
              ],
              "as": "cost2",
              "cast": {
                "type": "cds.Integer64"
              }
            },
            {
              "func": "count",
              "args": [
                "*"
              ],
              "as": "cnt",
              "cast": {
                "type": "cds.Integer"
              }
            }
          ],
          "groupBy": [
            {
              "ref": [
                "a",
                "asst_type2"
              ]
            }
          ]
        }
      },
      "elements": {
        "asst_type2": {
          "type": "cds.String",
          "length": 10
        },
        "cost1": {
          "@Core.Computed": true,
          "type": "cds.Integer64"
        },
        "cost2": {
          "@Core.Computed": true,
          "type": "cds.Integer64"
        },
        "cnt": {
          "@Core.Computed": true,
          "type": "cds.Integer"
        }
      }
    },
